      ],
      "minify": {
        "jquery-ui": [
            "./node_modules/jquery-ui/themes/base/*.css"
        ]
      },
      "fonts": [
        "./node_modules/font-awesome/fonts/*",
        "./node_modules/bootstrap/fonts/*"
      ],
      "images": {
        "css/jquery-ui/images": [
            "./node_modules/jquery-ui-dist/images/*"
